
directories:
    - src
    - tests
    - build
    - stubs
    - //

dictionariesByFileName:
    */Ansi.php: skip
    */debug-config.php: skip

dictionariesByFileExtension:
    neon: en
    php: en php
    phpt: en php

dictionaryDirectories:
    - vendor/spell-checker/spell-checker
    - vendor/spell-checker/dictionary-en
    - build

checkLocalIgnores: true

ignoreUrls: true
